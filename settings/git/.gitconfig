[alias]
    s = status
    ss = status -s
    ck = checkout
    cp = cherry-pick
    br = branch -vv

	tree = log --all --graph --decorate --pretty=oneline --abbrev-commit
    lg = log --graph --decorate --oneline
    sync-force = !git branch | xargs git branch -D

    alias = "!git config --list | egrep '^alias.+' | sed -e 's/^alias\\.//' | sed -e 's/^[^=]*=/\\'$'\\033[31m&\\033[(B\\033[m/'"
[user]
	email = metalg0su@gmail.com
	name = Seongjun Ji
[core]
	editor = vim
[difftool]
    prompt = true
[push]
	default = current
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
	conflictstyle = diff3
[color]
	ui = auto
	branch = auto
	status = auto
[commit]
	verbose = true
