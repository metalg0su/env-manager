[alias]
	tree = log --all --graph --decorate --pretty=oneline --abbrev-commit
    tree-all = !git log --graph --decorate $(git rev-list -g --all)
    s = status
    sync-force = !git branch | xargs git branch -D
    ck = checkout
    cp = cherry-pick
    tagprune = fetch origin --prune +refs/tags/*:refs/tags/* 
[user]
	email = metalg0su@gmail.com
	name = Seongjun Ji
[core]
	editor = vim
[difftool]
    prompt = true
[push]
	default = current
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
	conflictstyle = diff3
[color]
	ui = auto
	branch = auto
	status = auto
[commit]
	verbose = true
